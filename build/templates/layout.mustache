    <!-- GOV UK styles -->
    <link href="/assets/stylesheets/govuk-template.css" media="screen" rel="stylesheet" type="text/css"/>
    <link href="/assets/stylesheets/fonts.css" media="all" rel="stylesheet" type="text/css"/>

    <!-- https://scotch.io/tutorials/all-favicons-for-all-devices-and-sizes -->
    <link rel="shortcut icon" sizes="16x16 24x24 32x32 48x48 64x64" href="/assets/images/favicon.ico">
    <link rel="apple-touch-icon" sizes="57x57" href="">
    <link rel="apple-touch-icon-precomposed" sizes="57x57" href="/assets/images/apple-touch-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="">
    <link rel="apple-touch-icon" sizes="114x114" href="">
    <link rel="apple-touch-icon" sizes="120x120" href="/assets/images/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="">
